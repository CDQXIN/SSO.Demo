@{
    ViewData["Title"] = "添加";
}
@using SSO.Demo.Web1.Model
@model UserParams
<form id="addForm" method="post" action="/user/add">
    <table>
        <tr>
            <td>
                用户名：
            </td>
            <td>
                @Html.EditorFor(a => a.UserName)
            </td>
        </tr>
        <tr>
            <td>
                密码：
            </td>
            <td>
                @Html.EditorFor(a => a.Password)
            </td>
        </tr>
    </table>
    <input type="submit" value="提交" />
</form>